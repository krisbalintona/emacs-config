alias cat kb/eshell-cat $1
alias la ls -la
alias ffo find-file-other-window nil
alias clear clear-scrollback
alias l ls -lh $*
alias ll ls -lah $*
alias d dired $1
alias ff find-file $1
alias q exit
alias mcd mkdir $1 && cd $1
alias stc sudo timeshift --create
alias stl sudo timeshift --list
alias stcc sudo timeshift --create --comment
alias pk eshell-up-peek
alias up eshell-up
